# ========================================
# FastFoodBike Frontend Configuration
# ========================================

# === ENVIRONNEMENT ===
VITE_ENV=development
VITE_DEBUG=true

# === API ===
# Development
VITE_API_URL=http://localhost:5000/api

# Production
# VITE_API_URL=https://api.fastfoodbike.com/api

# === PAIEMENT STRIPE ===
# https://stripe.com - Public Key
VITE_STRIPE_PUBLIC_KEY=pk_test_...

# === CARTES (Google Maps) ===
# https://console.cloud.google.com
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# === AUTHENTIFICATION ===
# Token storage
VITE_TOKEN_STORAGE=localStorage
# ou sessionStorage

# === ANALYTICS ===
# Google Analytics
VITE_GA_ID=

# Sentry Error Tracking
VITE_SENTRY_DSN=

# === FEATURES FLAGS ===
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_WALLET=false

# === APP VERSION ===
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=FastFoodBike

# === SOCKET.IO ===
VITE_SOCKET_URL=http://localhost:5000
VITE_SOCKET_AUTO_CONNECT=true

# === TIMEOUT ===
VITE_REQUEST_TIMEOUT=30000
VITE_DEBOUNCE_DELAY=300
