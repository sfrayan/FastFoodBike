{
  "name": "fastfoodbike-frontend",
  "version": "1.0.0",
  "description": "FastFoodBike Web Application",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --max-warnings 0",
